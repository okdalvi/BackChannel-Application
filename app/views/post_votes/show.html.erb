<% provide(:title, 'People who have voted') %>
<ul id="posts_list">
    <div class="post_message_container">
      <h3><% @post_votes.each do |v| %></h3>
            <li class="first">
                <h4><%= v.user.name%></h4>
                <div class="time_ago">
                  <%= time_ago_in_words(v.created_at)%> ago
                </div>
            </li>
        <% end %>
        </div>
</ul>
<%= link_to 'Edit', edit_post_vote_path(@post_vote) %> |
<%= link_to 'Back', post_votes_path %>
